{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimiting": {
    "Enabled": false,
    "PerMinute": 60,
    "PerHour": 1000
  },
  "Authentication": {
    "Enabled": false,
    "Schemes": {
      "ApiKey": {
        "Enabled": true,
        "HeaderName": "X-API-Key"
      },
      "JwtBearer": {
        "Enabled": false,
        "Authority": "",
        "Audience": "",
        "RequireHttpsMetadata": true
      }
    },
    "ApiKeys": {
      "dev-key": "Developer"
    }
  }
}
